/* 1像素边框 */
.bt1 {
    position: relative;
}
.bt1::before {
    content: "";
    position: absolute;
    background-color: #E6E6E6;
    display: block;
    top: 0;
    left: 0;
    right: auto;
    height: 1px;
    width: 100%;
    transform-origin: 0 0;
    transform: scaleY(.5);
}
  
.bd1 {
    position: relative;
}
.bd1::before {
    content: "";
    position: absolute;
    background-color: #E6E6E6;
    display: block;
    bottom: 0;
    left: 0;
    right: auto;
    height: 1px;
    width: 100%;
    transform-origin: 0 0;
    transform: scaleY(.5);
}
  
.bl1 {
    position: relative;
}
.bl1::after {
    content: "";
    position: absolute;
    background-color: #E6E6E6;
    display: block;
    bottom: 0;
    left: 0;
    right: auto;
    height: 100%;
    width: 1px;
    transform-origin: 0 0;
    transform: scaleX(.5);
}
  
.br1 {
    position: relative;
}
.br1::after {
    content: "";
    position: absolute;
    background-color: #E6E6E6;
    display: block;
    bottom: 0;
    left: auto;
    right: 0;
    height: 100%;
    width: 1px;
    transform: scaleX(.5);
    transform-origin: 0 0;
}

