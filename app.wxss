button[type=none]{
    width: inherit;
    height: inherit;
    display: inherit;
    text-align: inherit;
    position: static;
    margin-left: inherit;
    margin-right: inherit;
    box-sizing: inherit;
    line-height: inherit;
    overflow: inherit;
    color: inherit;
    background-color: inherit;
    border-radius: inherit;
    -webkit-tap-highlight-color: inherit;
    padding-left: inherit;
    padding-right: inherit;
    font-size: inherit;
    text-decoration: inherit;
}
button[type=none]::after{
    content: none;
}

