/* pages/vip/vip-center.wxss */
page{
    background: #33302E;
}
.i-img{
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    /* overflow: hidden; */
}
.sp{
    display: inline-block;
    word-break: break-all;
    flex-direction: column;
}
.open-vip{
    width: 162rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 24rpx;
    border-radius: 24rpx;
    padding: 0;
}
.list{
    display: flex;
    flex-wrap: wrap;
}
.list-bd{
    width: 139rpx;
    height: 140rpx;
    display: inline-block;
    border: 1rpx solid rgba(245,206,147,1);
    border-radius:6rpx;
    text-align: center;
    padding: 10rpx;
    margin-right: 7rpx;
    margin-bottom: 7rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}
.c-F5CE93{
    color: #F5CE93;
}

.mt50{
    margin-top: 50rpx;
}
.pass{
    border-radius:16rpx;
}
.pass-title{
    height: 100rpx;
    line-height: 100rpx;
}
.pass-items{
    border:1rpx solid #00A653;
    border-radius:12rpx;
    height: 160rpx;
}
.pass-items:not(:last-child) {
    margin-bottom: 20rpx;
}
.pass-btn{
    width: 140rpx;
    height: 56rpx;
    line-height: 56rpx;
}
.w-100{
    width: 100%;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    word-wrap: break-word;
}
.pic-default-new{
    border-radius: 50%;
}